#include <string>
#include <vector>
#include<algorithm>

using namespace std;

int solution(vector<vector<int>> sizes) {
    int max1=0,max2=0;
    for(int i=0;i<sizes.size();i++){
        max1=max(max1,min(sizes[i][1],sizes[i][0]));
        max2=max(max2, max(sizes[i][1],sizes[i][0]));
    }
    return max1*max2;
}
